{
  "name": "FAPI API with DPoP",
  "slug": "fapi-api",
  "api_id": "fapi-api",
  "org_id": "default",
  "use_keyless": false,
  "auth": {
    "auth_header_name": "Authorization"
  },
  "definition": {
    "location": "header",
    "key": "x-api-version"
  },
  "version_data": {
    "not_versioned": true,
    "versions": {
      "Default": {
        "name": "Default",
        "use_extended_paths": true
      }
    }
  },
  "proxy": {
    "listen_path": "/fapi/",
    "target_url": "https://your-upstream-api.com/",
    "strip_listen_path": true
  },
  "jwt_signing_method": "rsa",
  "jwt_source": "https://your-jwks-url/.well-known/jwks.json",
  "jwt_identity_base_field": "sub",
  "jwt_policy_field_name": "pol",
  "custom_middleware": {
    "pre": [
      {
        "name": "PreAuthCheck"
      }
    ],
    "post_key_auth": [
      {
        "name": "PostKeyAuth"
      }
    ],
    "driver": "grpc"
  },
  "enable_jwt": true,
  "jwt_client_base_field": "client_id",
  "jwt_disable_issued_at_validation": false,
  "jwt_disable_expires_at_validation": false,
  "jwt_skip_kid": false,
  "active": true
}
